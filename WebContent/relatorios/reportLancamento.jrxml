	<queryString>
		<![CDATA[SELECT L.COMENTARIO, L.TIPOLANCAMENTO, L.DATA, LP.QUANTIDADE, LP.VALOR, P.IDPRODUTO, P.DESCRICAO, U.NOME FROM LANCAMENTO L
INNER JOIN LANCAMENTOPRODUTO LP ON LP.LANCAMENTO_ID = L.IDLANCAMENTO
INNER JOIN PRODUTO P ON P.IDPRODUTO = LP.PRODUTO
INNER JOIN USUARIO U
WHERE L.TIPOLANCAMENTO LIKE $P{PARAM_TIPO}]]>
