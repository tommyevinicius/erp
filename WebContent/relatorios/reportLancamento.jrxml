	<subDataset name="LANCAMENTOS">
		<queryString language="SQL">
			<![CDATA[SELECT DISTINCT P.IDPRODUTO, L.DATA, P.DESCRICAO, LP.QUANTIDADE AS QTDE, LP.VALOR AS VALOR, (LP.QUANTIDADE * LP.VALOR) AS TOTAL FROM LANCAMENTO L
INNER JOIN LANCAMENTOPRODUTO LP ON LP.LANCAMENTO_ID = L.IDLANCAMENTO
INNER JOIN PRODUTO P ON P.IDPRODUTO = LP.PRODUTO
WHERE L.TIPOLANCAMENTO = 'ENTRADA'
ORDER BY P.IDPRODUTO]]>
		</queryString>
		<field name="IDPRODUTO" class="java.lang.Long">
			<fieldDescription><![CDATA[]]></fieldDescription>
		</field>
		<field name="DATA" class="java.sql.Timestamp">
