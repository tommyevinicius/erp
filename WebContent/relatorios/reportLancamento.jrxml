	<subDataset name="TABLE">
		<queryString>
			<![CDATA[SELECT L.IDLANCAMENTO, L.DATA, P.DESCRICAO, U.NOME, LP.QUANTIDADE, LP.VALOR, (LP.QUANTIDADE * LP.VALOR) AS TOTALPRODUTO, L.TIPOLANCAMENTO FROM LANCAMENTO L
INNER JOIN LANCAMENTOPRODUTO LP ON LP.LANCAMENTO_ID = L.IDLANCAMENTO
INNER JOIN PRODUTO P ON P.IDPRODUTO = LP.PRODUTO
INNER JOIN USUARIO U ON U.IDUSUARIO = L.USUARIO_ID
WHERE 1 = 1
ORDER BY L.TIPOLANCAMENTO, L.IDLANCAMENTO]]>
		</queryString>
		<field name="IDLANCAMENTO" class="java.lang.Long">
			<fieldDescription><![CDATA[]]></fieldDescription>
		</field>
		<field name="DATA" class="java.sql.Timestamp">
			<fieldDescription><![CDATA[]]></fieldDescription>
