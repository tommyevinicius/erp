<?xml version="1.0" encoding="UTF-8"?>
<page xmlns="http://jboss.org/schema/seam/pages" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://jboss.org/schema/seam/pages http://jboss.org/schema/seam/pages-2.3.xsd"
	view-id="/pages/perfil/perfilManter.xhtml" login-required="true">

	<begin-conversation join="true" />
	<action execute="#{redirect.captureCurrentView}" />

	<navigation>
		<rule if-outcome="perfilIncluir">
			<redirect view-id="/pages/perfil/perfilIncluir.xhtml"/>
		</rule>

		<rule if-outcome="perfilAlterar">
			<redirect view-id="/pages/perfil/perfilAlterar.xhtml"/>
		</rule>
	</navigation>

</page>
