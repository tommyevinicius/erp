		</field>
		<field name="VALOR" class="java.lang.Double">
			<fieldDescription><![CDATA[]]></fieldDescription>
		</field>
		<field name="TOTALPRODUTO" class="java.lang.Double"/>
		<field name="TIPOLANCAMENTO" class="java.lang.String">
			<fieldDescription><![CDATA[]]></fieldDescription>
		</field>
	</subDataset>
	<subDataset name="SOMATORIO">
		<queryString language="SQL">
			<![CDATA[SELECT
     SUM(TOTAL)
FROM (
        SELECT SUM(LP.QUANTIDADE * LP.VALOR) AS TOTAL FROM LANCAMENTO L
		INNER JOIN LANCAMENTOPRODUTO LP ON LP.LANCAMENTO_ID = L.IDLANCAMENTO
		WHERE L.TIPOLANCAMENTO = 'ENTRADA'
    UNION ALL
        SELECT SUM(-LP.QUANTIDADE * LP.VALOR) AS TOTAL FROM LANCAMENTO L
		INNER JOIN LANCAMENTOPRODUTO LP ON LP.LANCAMENTO_ID = L.IDLANCAMENTO
		WHERE L.TIPOLANCAMENTO = 'SAIDA'
)  AS T]]>
		</queryString>
		<field name="SUM(TOTAL)" class="java.lang.Double"/>
	</subDataset>
	<queryString>
		<![CDATA[SELECT L.IDLANCAMENTO, L.DATA, P.DESCRICAO, U.NOME, LP.QUANTIDADE, LP.VALOR, (LP.QUANTIDADE * LP.VALOR) AS TOTALPRODUTO, L.TIPOLANCAMENTO FROM LANCAMENTO L
INNER JOIN LANCAMENTOPRODUTO LP ON LP.LANCAMENTO_ID = L.IDLANCAMENTO
INNER JOIN PRODUTO P ON P.IDPRODUTO = LP.PRODUTO
INNER JOIN USUARIO U ON U.IDUSUARIO = L.USUARIO_ID
WHERE 1 = 1]]>
	</queryString>
